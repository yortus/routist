  },
  "homepage": "https://github.com/yortus/routist#readme",
  "dependencies": {},
  "devDependencies": {
    "chai": "^3.2.0",
    "mocha": "^2.2.5",
    "ncp": "^2.0.0",
    "pegjs": "^0.9.0",
    "rimraf": "^2.5.2",
    "typescript": "^1.9.0-dev.20160224"
  }

{
  "name": "routist",
  "version": "0.0.5",
  "description": "routist",
  "readmeFilename": "README.md",
  "main": "src/index",
  "typings": "src/index.ts",
  "scripts": {
    "build": "npm run build-ts && npm run build-peg && npm run make-dist",
    "clean": "rimraf built/ dist/",
    "prepublish": "npm run build",
    "test": "node ./scripts/run-mocha-tests",
    "perf": "node ./scripts/run-perf-tests",

    "build-ts": "tsc",
    "build-peg": "pegjs src/pattern/pattern-source-grammar.peg built/src/pattern/pattern-source-grammar.js",
    "make-dist": "ncp built/src dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yortus/routist.git"
  },
  "keywords": [
    "router",
    "routing"
  ],
